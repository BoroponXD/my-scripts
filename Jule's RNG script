if _G.AUTO_COLLECT_LOADED and not _G.DEBUG_MODE then
    return
end

pcall(function() getgenv().AUTO_COLLECT_LOADED = true end)

if _G.AUTO_COLLECT_EXECUTED then
    return
end
_G.AUTO_COLLECT_EXECUTED = true

local cloneref = cloneref or function(o) return o end
local COREGUI = cloneref(game:GetService("CoreGui"))
local Players = cloneref(game:GetService("Players"))
local TeleportService = cloneref(game:GetService("TeleportService"))

if not game:IsLoaded() then
    local msg = Instance.new("Message")
    msg.Parent = COREGUI
    msg.Text = "AutoCollect Script is waiting for the game to load..."
    game.Loaded:Wait()
    msg:Destroy()
end

local SCRIPT_URL = "https://raw.githubusercontent.com/BoroponXD/Roblox-Script/refs/heads/main/Jule's%20private%20handler"

local success, err = pcall(function()
    loadstring(game:HttpGet(SCRIPT_URL))()
end)

if not success then
    warn("Error while loading AutoCollect:", err)
end

if syn and syn.queue_on_teleport then
    syn.queue_on_teleport(('loadstring(game:HttpGet("%s"))()'):format(SCRIPT_URL))
elseif queue_on_teleport then
    queue_on_teleport(('loadstring(game:HttpGet("%s"))()'):format(SCRIPT_URL))
end
